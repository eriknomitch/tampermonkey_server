#= require utility
#= require_self
#= require ubermonkey

@UM =
  env: "<%= Rails.env %>"
  remote:
    suffix: "/assets"
  use:
    stylesheet: true
  restartOn:
    pushState: true

[UM.domain, UM.remote.protocol] = switch UM.env
  when "development"
    ["development.ubermonkey-server.com:3000", "http"]
  when "production"
    ["ubermonkey-server.herokuapp.com", "https"]

trace UM
