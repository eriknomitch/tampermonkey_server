#= require utility
#= require_self
#= require uber_monkey

@UM =
  env: "<%= Rails.env %>"
  remote:
    suffix: "/assets"
  use:
    stylesheet: true

switch UM.env
  when "development"
    UM.domain = "development.ubermonkey-server.com:3000"
    UM.remote.protocol = "http"
  when "production"
    UM.domain = "ubermonkey-server.herokuapp.com"
    UM.remote.protocol = "https"

